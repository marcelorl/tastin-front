    "google-map-react": "^1.1.2",
    "lodash": "^4.17.4",
    "material-ui": "0.20.2",
    "prop-types": "^15.5.8",
    "react": "16.8.1",
    "react-addons-test-utils": "15.6.2",
    "react-dom": "16.8.1",
    "react-redux": "^6.0.0",
